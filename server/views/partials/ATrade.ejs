<% data.forEach(function(d) { %>

<p class="singleListingTitle"><span class="brand"><%= d.brand %></span> <%= d.title %> (<%= d.condition %>)<br>
<span class="listingBy">Posted by <%= d.username %> on <%= d.date_created %>
</p>
<p>
<img src="<%= d.image1 %>"><br>
<% if(d.image2 != "http://via.placeholder.com/400x400") { %>
<img src="<%= d.image2 %>"><br>
<% } %>

<% if(d.image3 != "http://via.placeholder.com/400x400") { %>
<img src="<%= d.image3 %>"><br>
<% } %>

<% if(d.image4 != "http://via.placeholder.com/400x400") { %>
<img src="<%= d.image4 %>"><br>
<% } %>

<% if(d.image5 != "http://via.placeholder.com/400x400") { %>
<img src="<%= d.image5 %>">
<% } %>
</p>

<p class="contactOwner"><i class="fa fa-home" aria-hidden="true"></i> <b>Located in</b> <%= d.city %>, <%= d.state %><br>
<span class="tradeInfo">Trade Information</span>
<p>Trading a/n <strong><%= d.size %></strong></p>
<p>Looking for a/n <strong><%= d.whatsize %></strong></p>
<p>Willing to ship? <strong><%= d.ship %></strong></p>
<p>Willing to meetup? <strong><%= d.meetup %></strong></p>
<p>Willing to throw cash?
<% if(d.cash === 0) { %>
<span class="noMoney">No</span>
<% } else { %>
<span class="money">$<%= d.cash %></span>
<% } %>
</p>
<p>
<a href="/browse/accept/<%= d.id %>"><button>Accept Trade</button></a><br>
<% if(user.username === d.username) { %>
    <a href="/browse/delete/<%= d.id %>"><button>Delete Trade</button></a><br>
<% } %>
<span class="acceptThisListing">Accept this trade, to view the owners contact information.</span>
</p>

<% }) %>